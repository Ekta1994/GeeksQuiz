{
	"manifest_version": 2,
	"name":"GeeksQuiz",
	"description": "Generate random quiz from geeksquiz.",
	"version": "1.0",
	"permissions": [
		"http://*/",
		"https://*/",
		"storage"
  	],
	"browser_action": {
		"default_icon": "images/logo.png",
		"default_popup": "popup.html",
		"default_title": "GeeksQuiz"
	},
	"content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'"
}