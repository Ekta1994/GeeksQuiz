{
	"manifest_version": 2,
	"name":"GeeksQuiz",
	"description": "Generate random quiz from geeksquiz.",
	"version": "1.0",
	"app": {
		"background": {
      		"scripts": ["js/background.js"]
    	},
    	"permissions": ["<all_urls>"]
	},
	"permissions": [
		"http://*/",
		"https://*/",
		"<all_urls>"
	],
	"icons": { "16": "images/logo.png", "128": "images/logo128.png" }
}